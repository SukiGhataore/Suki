pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building third run'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing third run'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying third run'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing third run'
            }
        }
    }
}
